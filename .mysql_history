CREATE TABLE IF NOT EXISTS `etl_servicios` (
  `fecha_consulta` datetime NOT NULL,
  `servicio` varchar(30) NOT NULL,
  `nuevas` int(11) NOT NULL,
  `activas` int(11) NOT NULL,
  `restauradas` int(11) NOT NULL,
  `delegadas` int(11) NOT NULL,
  `tipo` varchar(15) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE IF NOT EXISTS `etl_servicios` (   `fecha_consulta` datetime NOT NULL,   `servicio` varchar(30) NOT NULL,   `nuevas` int(11) NOT NULL,   `activas` int(11) NOT NULL,   `restauradas` int(11) NOT NULL,   `delegadas` int(11) NOT NULL,   `tipo` varchar(15) NOT NULL ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
