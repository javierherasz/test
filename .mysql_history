CREATE DATABASE `informespre` DEFAULT CHARACTER SET latin1 COLLATE latin1_swedish_ci;
USE `informespre`;
CREATE TABLE IF NOT EXISTS `etl_incidencias` (
  `id` int(6) NOT NULL,
  `servicio` varchar(15) NOT NULL,
  `severidad` varchar(15) NOT NULL,
  `tipo` varchar(10) NOT NULL,
  `sla_restaurado` varchar(30) NOT NULL,
  `fecha_apertura` datetime NOT NULL,
  `fecha_restaurado` datetime NOT NULL,
  `fuerasla` int(11) NOT NULL,
  `tiempo_retenida` varchar(30) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE IF NOT EXISTS `etl_incidencias` (   `id` int(6) NOT NULL,   `servicio` varchar(15) NOT NULL,   `severidad` varchar(15) NOT NULL,   `tipo` varchar(10) NOT NULL,   `sla_restaurado` varchar(30) NOT NULL,   `fecha_apertura` datetime NOT NULL,   `fecha_restaurado` datetime NOT NULL,   `fuerasla` int(11) NOT NULL,   `tiempo_retenida` varchar(30) NOT NULL ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
